#####
# DOCKER provider
#####
providers:
  docker:
    swarmMode: true
    exposedbydefault: false
  {% raw %}
    defaultRule: "PathPrefix(`/{{ .Name }}`)"
  {% endraw %}

#####
# OPS configs
#####
api:
  insecure: {{ insecure }}
  debug: {{ debug }}

log:
  filePath: "/traefik.log"

accessLog:
  filePath: "/traefik-access.log"